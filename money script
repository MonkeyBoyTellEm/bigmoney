local remote = game.ReplicatedStorage.Remotes["Events"]
local pass = getrenv()._G.Pass
local BoneAnimations = player.Backpack.Main.XSansMoves.ModuleScript.Animations.NormalCombat
local XSansKnife = player.Backpack.Main.XSansMoves.ModuleScript.Animations.KnifeCombat
